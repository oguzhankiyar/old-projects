@model bgk_etkinlik_gorevli

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<div class="title">Görevli Ekle</div>
<div class="content form">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
        <ul>
            <li>@Html.DisplayNameFor(model => model.UyeID)</li>
            <li>@Html.DropDownList("UyeID", String.Empty)</li>
            <li>@Html.ValidationMessageFor(model => model.UyeID)</li>
        </ul>
        <ul>
            <li>@Html.DisplayNameFor(model => model.GorevID)</li>
            <li>@Html.DropDownList("GorevID", String.Empty)</li>
            <li>@Html.ValidationMessageFor(model => model.GorevID)</li>
        </ul>
        @Html.HiddenFor(model => model.EtkinlikID)
    }
</div>